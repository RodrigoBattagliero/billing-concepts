{% extends 'base.html.twig' %}

{% block title %}IvaApplication index{% endblock %}

{% block body %}
    <h1>IvaApplication index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Code</th>
                <th>IvaApplication</th>
                <th>Aliquot</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for iva_application in iva_applications %}
            <tr>
                <td>{{ iva_application.id }}</td>
                <td>{{ iva_application.code }}</td>
                <td>{{ iva_application.ivaApplication }}</td>
                <td>{{ iva_application.aliquot }}</td>
                <td>
                    <a href="{{ path('app_iva_application_show', {'id': iva_application.id}) }}">show</a>
                    <a href="{{ path('app_iva_application_edit', {'id': iva_application.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_iva_application_new') }}">Create new</a>
{% endblock %}
